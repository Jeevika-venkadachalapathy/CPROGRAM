#include<stdio.h>
int main(){
    int s1,s2,s3,s4,s5;
    scanf("%d %d %d %d %d",&s1,&s2,&s3,&s4,&s5);
    float per=( s1+s2+s3+s4+s5)/5.0;
    {
        if(per>=95)
        printf("Grade A");
        else if(per>=85)
        printf("Grade B");
        else if(per>=75)
        printf("Grade C");
        else if(per>=65)
        printf("Grade D");
        else if(per>=45)
        printf("Grade E");
        else
        printf("Grade F");
        
    }
}
